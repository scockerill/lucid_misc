        ],
        "summary": "Authenticate with the BuildingOS API",
        "parameters": [
          {
            "name": "client_id",
            "in": "query",
            "description": "client id provided by BuildingOS",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "client_secret",
            "in": "query",
            "description": "client secret provided by BuildingOS",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "grant_type",
            "in": "query",
            "description": "\\'client_credentials\\' is preferred",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Authentication response",
            "content": {
              "application/json": {
                "schema": {

    "/gateways/{id}/data": {
      "post": {
        "tags": [
          "gateways"
        ],
        "summary": "Post time series data to a gateway",
        "operationId": "postGatewayData",
        "description": "Use gateway id in the url.",
        "parameters": [
          {
            "$ref": "#/components/parameters/id"
          }
        ],
        "requestBody": {
          "description": "Post time series data to a gateway",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "meta": {
                    "type": "object",
                    "properties": {
                      "naive_timestamp_utc": {
                        "type": "boolean",
                        "default": true
                      }
                    }
                  },
                  "data": {
                    "type": "object",
                    "additionalProperties": {
                      "description": "Each key must be a meter uuid",
                      "type": "array",
                      "items": {
                        "type": "array",
                        "items": {
                          "anyOf": [
                            {
                              "type": "string",
                              "format": "date-time",
                              "description": "timestamp of data point"
                            },
                            {
                              "type": "number",
                              "description": "value of data point"
                            }
                          ]
                        }
                      }
                    }
                  }
                }
              },
              "examples": {
                "json": {
                  "summary": "Sample JSON request",
                  "value": "{ \"meta\": {\"naive_timestamp_utc\": true},\"data\": {\"d4632168e17511e581a75254006a6386\": [[\"2017-03-14T18:10:00Z\", 157.77], [\"2017-03-14T18:20:00Z\", 158.77], [\"2017-03-14T18:30:00Z\", 159.77], [\"2017-03-14T18:40:00Z\", 160.77], [\"2017-03-14T18:50:00Z\", 164.77], [\"2017-03-14T18:00:00Z\", 166.66]] } }"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Data accepted",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
